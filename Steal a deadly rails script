local TweenService = game:GetService("TweenService")
local RunService = game:GetService("RunService")
local StarterGui = game:GetService("StarterGui")
local player = game:GetService("Players").LocalPlayer
local playerGui = player:WaitForChild("PlayerGui")
local character = player.Character or player.CharacterAdded:Wait()
local humanoid = character:WaitForChild("Humanoid")
local hrp = character:WaitForChild("HumanoidRootPart")
local workspace = game:GetService("Workspace")

-- Function to flash button
local function flashButton(button)
    local originalColor = button.BackgroundColor3
    button.BackgroundColor3 = Color3.new(1, 1, 1) -- White flash
    task.wait(0.1)
    button.BackgroundColor3 = originalColor
end

-- Set WalkSpeed at start
humanoid.WalkSpeed = 35

-- ScreenGui
local screenGui = Instance.new("ScreenGui")
screenGui.Name = "MenuGui"
screenGui.ResetOnSpawn = false
screenGui.Parent = playerGui

-- Main frame
local mainFrame = Instance.new("Frame")
mainFrame.Size = UDim2.new(0, 200, 0, 300)
mainFrame.Position = UDim2.new(0.5, -100, 0.5, -150)
mainFrame.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
mainFrame.BorderSizePixel = 0
mainFrame.Active = true
mainFrame.Draggable = true
mainFrame.Parent = screenGui

-- Collapse button
local collapseButton = Instance.new("TextButton")
collapseButton.Size = UDim2.new(0, 30, 0, 30)
collapseButton.Position = UDim2.new(0, 0, 0, 0)
collapseButton.BackgroundTransparency = 1
collapseButton.Text = "▶"
collapseButton.TextColor3 = Color3.fromRGB(255, 255, 255)
collapseButton.Font = Enum.Font.SourceSansBold
collapseButton.TextSize = 20
collapseButton.Parent = mainFrame

-- Title label
local title = Instance.new("TextLabel")
title.Size = UDim2.new(1, -30, 0, 30)
title.Position = UDim2.new(0, 30, 0, 0)
title.BackgroundColor3 = Color3.fromRGB(10, 10, 10)
title.Text = "Steal a deadly rails"
title.TextColor3 = Color3.fromRGB(255, 255, 255)
title.Font = Enum.Font.SourceSansBold
title.TextSize = 18
title.TextXAlignment = Enum.TextXAlignment.Center
title.Parent = mainFrame

-- Anti slap toggle
local toggleFrame = Instance.new("Frame")
toggleFrame.Size = UDim2.new(1, 0, 0, 30)
toggleFrame.Position = UDim2.new(0, 0, 0, 35)
toggleFrame.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
toggleFrame.BorderSizePixel = 0
toggleFrame.Parent = mainFrame

local toggleLabel = Instance.new("TextLabel")
toggleLabel.Size = UDim2.new(1, -30, 1, 0)
toggleLabel.Position = UDim2.new(0, 0, 0, 0)
toggleLabel.BackgroundTransparency = 1
toggleLabel.Text = "anti slap while stealing"
toggleLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
toggleLabel.Font = Enum.Font.SourceSans
toggleLabel.TextSize = 16
toggleLabel.TextXAlignment = Enum.TextXAlignment.Center
toggleLabel.Parent = toggleFrame

local toggleButton = Instance.new("TextButton")
toggleButton.Size = UDim2.new(0, 20, 0, 20)
toggleButton.Position = UDim2.new(1, -25, 0.5, -10)
toggleButton.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
toggleButton.BorderSizePixel = 0
toggleButton.Text = ""
toggleButton.Parent = toggleFrame

local enabled = false
local movingConnection

toggleButton.MouseButton1Click:Connect(function()
    flashButton(toggleButton)
    enabled = not enabled

    if enabled then
        toggleButton.BackgroundColor3 = Color3.fromRGB(0, 170, 0)
        workspace.Gravity = 0
        hrp.CFrame = hrp.CFrame + Vector3.new(0, 30, 0)

        player.DevEnableMouseLock = true
        player.DevComputerCameraMode = Enum.DevCameraMode.LockCameraToDefault

        movingConnection = RunService.Heartbeat:Connect(function(dt)
            if not enabled or humanoid.Health <= 0 then
                if movingConnection then
                    movingConnection:Disconnect()
                    movingConnection = nil
                end
                return
            end

            local lookVector = workspace.CurrentCamera.CFrame.LookVector
            local horizontalVector = Vector3.new(lookVector.X, 0, lookVector.Z)

            if horizontalVector.Magnitude > 0 then
                horizontalVector = horizontalVector.Unit
                local speed = 80
                local moveDistance = speed * dt
                hrp.CFrame = hrp.CFrame + horizontalVector * moveDistance
            end
        end)
    else
        toggleButton.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
        workspace.Gravity = 196.2

        if movingConnection then
            movingConnection:Disconnect()
            movingConnection = nil
        end

        player.DevEnableMouseLock = false
        player.DevComputerCameraMode = Enum.DevCameraMode.Classic
    end
end)

-- Boost Steal toggle
local boostFrame = Instance.new("Frame")
boostFrame.Size = UDim2.new(1, 0, 0, 30)
boostFrame.Position = UDim2.new(0, 0, 0, 70)
boostFrame.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
boostFrame.BorderSizePixel = 0
boostFrame.Parent = mainFrame

local boostLabel = Instance.new("TextLabel")
boostLabel.Size = UDim2.new(1, -30, 1, 0)
boostLabel.Position = UDim2.new(0, 0, 0, 0)
boostLabel.BackgroundTransparency = 1
boostLabel.Text = "Boost Steal"
boostLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
boostLabel.Font = Enum.Font.SourceSans
boostLabel.TextSize = 16
boostLabel.TextXAlignment = Enum.TextXAlignment.Center
boostLabel.Parent = boostFrame

local boostButton = Instance.new("TextButton")
boostButton.Size = UDim2.new(0, 20, 0, 20)
boostButton.Position = UDim2.new(1, -25, 0.5, -10)
boostButton.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
boostButton.BorderSizePixel = 0
boostButton.Text = ""
boostButton.Parent = boostFrame

local boostEnabled = false
local boostConnection

boostButton.MouseButton1Click:Connect(function()
    flashButton(boostButton)
    boostEnabled = not boostEnabled

    if boostEnabled then
        boostButton.BackgroundColor3 = Color3.fromRGB(0, 170, 0)
        boostConnection = RunService.Heartbeat:Connect(function()
            if humanoid.WalkSpeed < 35 then
                humanoid.WalkSpeed = 35
            end
        end)
    else
        boostButton.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
        if boostConnection then
            boostConnection:Disconnect()
            boostConnection = nil
        end
    end
end)

-- Tween steal Outside button
local tweenFrame = Instance.new("Frame")
tweenFrame.Size = UDim2.new(1, 0, 0, 30)
tweenFrame.Position = UDim2.new(0, 0, 0, 105)
tweenFrame.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
tweenFrame.BorderSizePixel = 0
tweenFrame.Parent = mainFrame

local tweenLabel = Instance.new("TextLabel")
tweenLabel.Size = UDim2.new(1, -10, 1, 0)
tweenLabel.Position = UDim2.new(0, 0, 0, 0)
tweenLabel.BackgroundTransparency = 1
tweenLabel.Text = "Tween steal Outside"
tweenLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
tweenLabel.Font = Enum.Font.SourceSans
tweenLabel.TextSize = 14
tweenLabel.TextXAlignment = Enum.TextXAlignment.Center
tweenLabel.Parent = tweenFrame

local tweenButton = Instance.new("TextButton")
tweenButton.Size = UDim2.new(1, 0, 1, 0)
tweenButton.Position = UDim2.new(0, 0, 0, 0)
tweenButton.BackgroundTransparency = 1
tweenButton.Text = ""
tweenButton.Parent = tweenFrame

tweenButton.MouseButton1Click:Connect(function()
    flashButton(tweenButton)
    local targetPos = Vector3.new(62.784, 13.630, 129.867)
    local distance = (hrp.Position - targetPos).Magnitude
    local time = distance / 80

    local tween = TweenService:Create(
        hrp,
        TweenInfo.new(time, Enum.EasingStyle.Linear),
        {CFrame = CFrame.new(targetPos)}
    )
    tween:Play()

    tween.Completed:Connect(function()
        StarterGui:SetCore("SendNotification", {
            Title = "Notification",
            Text = "successfully bypassed",
            Duration = 3
        })
    end)
end)

-- Noclip toggle
local noclipFrame = Instance.new("Frame")
noclipFrame.Size = UDim2.new(1, 0, 0, 30)
noclipFrame.Position = UDim2.new(0, 0, 0, 140)
noclipFrame.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
noclipFrame.BorderSizePixel = 0
noclipFrame.Parent = mainFrame

local noclipLabel = Instance.new("TextLabel")
noclipLabel.Size = UDim2.new(1, -30, 1, 0)
noclipLabel.Position = UDim2.new(0, 0, 0, 0)
noclipLabel.BackgroundTransparency = 1
noclipLabel.Text = "Noclip"
noclipLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
noclipLabel.Font = Enum.Font.SourceSans
noclipLabel.TextSize = 16
noclipLabel.TextXAlignment = Enum.TextXAlignment.Center
noclipLabel.Parent = noclipFrame

local noclipButton = Instance.new("TextButton")
noclipButton.Size = UDim2.new(0, 20, 0, 20)
noclipButton.Position = UDim2.new(1, -25, 0.5, -10)
noclipButton.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
noclipButton.BorderSizePixel = 0
noclipButton.Text = ""
noclipButton.Parent = noclipFrame

local noclipEnabled = false
local noclipConnection

noclipButton.MouseButton1Click:Connect(function()
    flashButton(noclipButton)
    noclipEnabled = not noclipEnabled

    if noclipEnabled then
        noclipButton.BackgroundColor3 = Color3.fromRGB(0, 170, 0)
        noclipConnection = RunService.Stepped:Connect(function()
            for _, v in pairs(character:GetDescendants()) do
                if v:IsA("BasePart") and v.CanCollide == true then
                    v.CanCollide = false
                end
            end
        end)
    else
        noclipButton.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
        if noclipConnection then
            noclipConnection:Disconnect()
            noclipConnection = nil
        end
        for _, v in pairs(character:GetDescendants()) do
            if v:IsA("BasePart") then
                v.CanCollide = true
            end
        end
    end
end)

-- Destroy GUI button
local destroyFrame = Instance.new("Frame")
destroyFrame.Size = UDim2.new(1, 0, 0, 30)
destroyFrame.Position = UDim2.new(0, 0, 0, 175)
destroyFrame.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
destroyFrame.BorderSizePixel = 0
destroyFrame.Parent = mainFrame

local destroyLabel = Instance.new("TextLabel")
destroyLabel.Size = UDim2.new(1, -10, 1, 0)
destroyLabel.Position = UDim2.new(0, 0, 0, 0)
destroyLabel.BackgroundTransparency = 1
destroyLabel.Text = "Destroy GUI"
destroyLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
destroyLabel.Font = Enum.Font.SourceSans
destroyLabel.TextSize = 14
destroyLabel.TextXAlignment = Enum.TextXAlignment.Center
destroyLabel.Parent = destroyFrame

local destroyButton = Instance.new("TextButton")
destroyButton.Size = UDim2.new(1, 0, 1, 0)
destroyButton.Position = UDim2.new(0, 0, 0, 0)
destroyButton.BackgroundTransparency = 1
destroyButton.Text = ""
destroyButton.Parent = destroyFrame

destroyButton.MouseButton1Click:Connect(function()
    flashButton(destroyButton)
    screenGui:Destroy()
end)

-- Footer label
local footer = Instance.new("TextLabel")
footer.Size = UDim2.new(1, 0, 0, 30)
footer.Position = UDim2.new(0, 0, 1, -30)
footer.BackgroundColor3 = Color3.fromRGB(10, 10, 10)
footer.Text = "By Rapture\nYouTube channel: Raptureofficialhack2"
footer.TextColor3 = Color3.fromRGB(255, 255, 255)
footer.Font = Enum.Font.SourceSans
footer.TextSize = 14
footer.TextWrapped = true
footer.TextYAlignment = Enum.TextYAlignment.Center
footer.Parent = mainFrame

-- Collapse logic
local isCollapsed = false
local collapsedHeight = 30
local expandedHeight = 300

collapseButton.MouseButton1Click:Connect(function()
    isCollapsed = not isCollapsed

    if isCollapsed then
        collapseButton.Text = "▼"
        local tween = TweenService:Create(mainFrame, TweenInfo.new(0.3, Enum.EasingStyle.Sine, Enum.EasingDirection.Out), {
            Size = UDim2.new(0, 200, 0, collapsedHeight)
        })
        tween:Play()

        toggleFrame.Visible = false
        boostFrame.Visible = false
        tweenFrame.Visible = false
        noclipFrame.Visible = false
        destroyFrame.Visible = false
        footer.Visible = false
    else
        collapseButton.Text = "▶"
        local tween = TweenService:Create(mainFrame, TweenInfo.new(0.3, Enum.EasingStyle.Sine, Enum.EasingDirection.Out), {
            Size = UDim2.new(0, 200, 0, expandedHeight)
        })
        tween:Play()

        toggleFrame.Visible = true
        boostFrame.Visible = true
        tweenFrame.Visible = true
        noclipFrame.Visible = true
        destroyFrame.Visible = true
        footer.Visible = true
    end
end)

-- Refresh on respawn
player.CharacterAdded:Connect(function(char)
    character = char
    humanoid = char:WaitForChild("Humanoid")
    hrp = char:WaitForChild("HumanoidRootPart")
    humanoid.WalkSpeed = 35
end)
